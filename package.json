{
  "name": "bilan_next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:vercel": "npx prisma generate && npx prisma migrate deploy && npx prisma db push --force-reset && npx prisma db seed && next build",
    "start": "next start",
    "start:migrate:prod": "node server.js",
    "lint": "next lint",
    "docker:dev": "npm run dev",
    "docker:compose:dev": "docker-compose down && docker compose -f compose.dev.yaml up --build",
    "docker:compose:prod": "docker-compose down && docker compose -f compose.yaml up --build",
    "docker:compose:db:only": "docker-compose down && docker compose -f compose.db.yaml up -d --build",
    "docker:compose:db": "docker-compose down && docker compose -f compose.db.yaml up -d --build && npm run dev",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
